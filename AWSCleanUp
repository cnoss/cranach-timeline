pipeline {
  agent any
   triggers {
      parameterizedCron('0 0 * * 0')
  }
  stages {
    stage('Checkout') {
      steps {
        checkout scm
      }
    }
    stage('CleanUp') {
      steps {
        nodejs(nodeJSInstallationName: 'DefaultNodeJs', configId: 'abdc70c6-1b44-4c3c-90d9-86e2fb1da02f') {
          sh 'npm install && npm run aws-clear'
        }
      }
    }
  }
}
